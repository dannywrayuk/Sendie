{
  "name": "sendie",
  "displayName": "sendie",
  "description": "A VSCode Extension for sending network requests",
  "version": "0.0.5",
  "icon": "images/icon.png",
  "publisher": "dannywrayuk",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sendie.openResponseDocument",
        "title": "Sendie: Open Response Document"
      },
      {
        "command": "sendie.updateResponseDocument",
        "title": "Sendie: Update Response Document"
      },
      {
        "command": "sendie.holdResponseDocument",
        "title": "Sendie: Hold Response Document"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sendie-sidebar-view",
          "title": "Sendie",
          "icon": "$(mail)"
        }
      ]
    },
    "views": {
      "sendie-sidebar-view": [
        {
          "type": "webview",
          "id": "sendie-sidebar",
          "name": "Sendie"
        }
      ]
    }
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack --watch",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "test:w": "jest --watch",
    "version": "echo $npm_package_version"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "glob": "^8.0.3",
    "http-status-codes": "^2.2.0",
    "node-fetch": "^3.3.0",
    "yaml": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dannywrayuk/sendie.git"
  }
}
